<h1>Books</h1>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Author</th>
      <th>Year</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @books.each do |book| %>
      <tr>
        <td><%= book.title %></td>
        <td><%= book.author %></td>
        <td><%= book.year %></td>
        <td>
          <%= link_to 'Edit', edit_book_path(book) %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= link_to 'New Book', new_book_path %>
